<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:KeyEvents.Common">
	<Color x:Key='SecondaryColor'>#FFD1A576</Color>
	<Color x:Key='AccentColor'>#FFFCF270</Color>
	<Color x:Key='MainColor'>#FF5798B0</Color>

	<SolidColorBrush x:Key='SecondaryColorBrush'
									 Color='{StaticResource SecondaryColor}' />
	<SolidColorBrush x:Key='AccentColorBrush'
									 Color='{StaticResource AccentColor}' />
	<SolidColorBrush x:Key='MainColorBrush'
									 Color='{StaticResource MainColor}' />
</ResourceDictionary>
